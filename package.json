{
  "name": "multi-token-standard",
  "version": "0.1.0",
  "description": "Multi Fungible Token contract for Ethereum",
  "repository": "https://github.com/horizon-games/multi-token-standard",
  "license": "MIT",
  "keywords": [
    "token",
    "ERC",
    "Fungible",
    "Non-Fungible",
    "ethereum",
    "horizongames"
  ],
  "scripts": {
    "build": "rm -rf ./build && truffle compile",
    "migrate": "truffle migrate --network ganache",
    "staging:migrate": "truffle migrate --network rinkeby",
    "ganache": "ganache-cli -d -e 1000000000 -g 1 -l 4000000000000000",
    "ganache:verbose": "ganache-cli -d -e 1000000000 -g 1 -l 4000000000000000 -v",
    "testrpc": "yarn ganache",
    "test": "NODE_ENV=test scripts/test.sh",
    "console": "truffle console",
    "coverage": "scripts/coverage.sh"
  },
  "dependencies": {
    "eth-sig-util": "^2.1.0",
    "ethers": "^4.0.0",
    "ethjs": "^0.4.0",
    "ethjs-abi": "^0.2.1",
    "ganache-cli": "6.1.8",
    "math": "^0.0.3",
    "openzeppelin-solidity": "1.12.0",
    "solc": "^0.4.25",
    "web3-utils": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.23.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.0.0",
    "chai-bignumber": "^2.0.0",
    "coveralls": "^3.0.2",
    "eslint": "5.6.1",
    "eslint-config-react-app": "^3.0.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "ethereumjs-testrpc": "^6.0.1",
    "ethereumjs-util": "^5.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-shared": "^0.2.0",
    "solidity-coverage": "^0.5.11",
    "solium": "^1.1.8",
    "truffle": "^4.1.14",
    "truffle-contract": "^3.0.6",
    "truffle-hdwallet-provider": "^0.0.6",
    "truffle-solidity-loader": "0.1.0",
    "url-loader": "1.1.1"
  }
